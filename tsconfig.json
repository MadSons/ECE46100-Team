{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "Phase 1/src/build",
    "rootDir": "Phase 1/src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "Phase 1/src/**/*.ts"
  ],
  "exclude": [
    "Phase 1/src/build",
    "Phase 1/src/node_modules"
  ]
}
